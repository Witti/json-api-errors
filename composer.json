  "require": {
    "php": "^7.4",
    "psr/http-factory": "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "WtfPhp\\JsonApiErrors\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "WtfPhp\\JsonApiErrors\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true,
    "process-timeout": 0
  }
